<div id="projects">
<h1 >Listing projects</h1>

<table>
  <tr>
    <th>Name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @projects.each do |project| %>
  <tr>
    <td><%= link_to project.name , project , :remote => true %></td>
    <!-- <td><%#= link_to 'Show',  %></td> -->
    <td><%= link_to 'Edit', edit_project_path(project) , :remote => true %></td>
    <td><%= link_to 'Destroy', project, method: :delete, data: { confirm: 'Are you sure?' },  :remote => true %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Project', new_project_path, :remote => true  %>
<%= link_to  'click' , ajaxaction_projects_path, :remote => true  %>
</div>